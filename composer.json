{
    "name": "silex/silex",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": ["microframework"],
    "homepage": "https://silex.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=8.2",
        "pimple/pimple": "^3.5",
        "symfony/event-dispatcher": "^7.1",
        "symfony/http-foundation": "^7.1",
        "symfony/http-kernel": "^7.1",
        "symfony/routing": "^7.1"
    },
    "require-dev": {
        "doctrine/dbal": "^2.2",
        "monolog/monolog": "^3.7",
        "symfony/asset": "^7.1",
        "symfony/browser-kit": "^7.1",
        "symfony/config": "^7.1",
        "symfony/css-selector": "^7.1",
        "symfony/doctrine-bridge": "^7.1",
        "symfony/dom-crawler": "^7.1",
        "symfony/expression-language": "^7.1",
        "symfony/finder": "^7.1",
        "symfony/form": "^7.1",
        "symfony/intl": "^7.1",
        "symfony/monolog-bridge": "^7.1",
        "symfony/options-resolver": "^7.1",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/process": "^7.1",
        "symfony/security-core": "^7.1",
        "symfony/security-csrf": "^7.1",
        "symfony/security-http": "^7.1",
        "symfony/serializer": "^7.1",
        "symfony/translation": "^7.1",
        "symfony/twig-bridge": "^7.1",
        "symfony/validator": "^7.1",
        "symfony/var-dumper": "^7.1",
        "symfony/web-link": "^7.1",
        "twig/twig": "^3.10"
    },
    "replace": {
        "silex/api": "self.version",
        "silex/providers": "self.version"
    },
    "autoload": {
        "psr-4": { "Silex\\": "src/Silex" }
    },
    "autoload-dev" : {
        "psr-4": { "Silex\\Tests\\" : "tests/Silex/Tests" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
